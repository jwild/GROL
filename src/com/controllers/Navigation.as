package com.controllers {		import flash.display.MovieClip;	import com.models.Model;	import flash.events.Event;	import flash.events.MouseEvent;		public class Navigation extends MovieClip{				private var m:Model = Model.instance;				public function Navigation() {			addEventListener(Event.ADDED_TO_STAGE, onLoad);		}				public function onLoad(e:Event):void{			trace("Class Navigation Loaded");			addEventListener(MouseEvent.CLICK, parseTouch);		}				public function parseTouch(e:MouseEvent):void{			trace(">>>"+e.target.name);			switch(e.target.name){				case "bttn_home" :					m.gotoScreen = "homeScreen";					break;				case "bttn_training" :					m.gotoScreen = "trainingScreen";					break;				case "bttn_arena" :					m.gotoScreen = "arenaScreen";					break;				case "bttn_armory" :					m.gotoScreen = "armoryScreenWeapons";					break;				case "bttn_collegium" :					m.gotoScreen = "collegiumScreen";					break;				default : break;			}		}	}	}